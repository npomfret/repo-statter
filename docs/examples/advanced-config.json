{
  "analysis": {
    "maxCommits": 5000,
    "bytesPerLineEstimate": 60,
    "timeSeriesHourlyThresholdHours": 24
  },
  "exclusions": {
    "patterns": [
      "**/vendor/**",
      "**/*.min.js",
      "**/generated/**"
    ]
  },
  "performance": {
    "cacheDirName": "my-custom-cache",
    "progressThrottleMs": 100,
    "cacheEnabled": true,
    "cacheVersion": "1.0"
  },
  "fileTypes": {
    "mappings": {
      ".vue": "Vue",
      ".svelte": "Svelte"
    }
  },
  "textAnalysis": {
    "stopWords": ["foo", "bar", "baz", "the", "is", "are"]
  },
  "fileHeat": {
    "recencyDecayDays": 60,
    "frequencyWeight": 0.3,
    "recencyWeight": 0.7,
    "maxFilesDisplayed": 100
  },
  "wordCloud": {
    "minWordLength": 4,
    "maxWords": 100,
    "minSize": 10,
    "maxSize": 80
  }
}